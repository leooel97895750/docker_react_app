{
    "compilerOptions": {
      "target": "ES6",
      "jsx": "react",
      "lib": ["dom", "dom.iterable", "esnext"],
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }